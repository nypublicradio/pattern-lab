{% if not_amp %}
  <!DOCTYPE html>
  <html class="{{ head.html_class }} no-js" lang="{{ head.language }}" {{ head.attr }}>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta charset="UTF-8">
    <title>{{ head.title }}</title>
    <meta name="description" content="{{ head.description }}">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" type="text/css" href="/css/themes/default/default.css?{{ cacheBuster }}" media="all">
    <!-- Begin Pattern Lab (Required for Pattern Lab to run properly) -->
    {#{ patternLabHead | raw }#}
    <!-- End Pattern Lab -->
    <link rel="icon" href="/favicon.png" />
  </head>
  <body class="{{ head.body_class }}">
    <div class="o-container" id="page-top">
{% endif %}
